{
  "name": "kaspa-rpc-client",
  "description": "Kaspa gRPC client for Node.js, written in Typescript",
  "version": "1.0.8",
  "main": "dist/index.js",
  "repository": "https://github.com/Cryptok777/kaspa-rpc-client.git",
  "author": "Cryptok777",
  "license": "MIT",
  "engines": {
    "node": ">=16.14"
  },
  "scripts": {
    "start:dev": "rm -rf ./dist && tsc-watch --onSuccess \"npm run copy-wasm\"",
    "build": "rm -rf ./dist && rm -f package-lock.json && npm install --also=dev && tsc && npm run copy-wasm",
    "prepublishOnly": "npm run build",
    "copy-wasm": "cp ./wasm/* ./dist/wasm",
    "test": "jest"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.13",
    "@types/lodash": "^4.14.192",
    "lodash": "^4.17.21",
    "ts-node": "^10.9.1",
    "ts-retry": "^4.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/sinon": "^10.0.13",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "sinon": "^15.0.3",
    "ts-jest": "^29.1.0",
    "ts-proto": "^1.146.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.0.3"
  },
  "files": [
    "dist",
    "client",
    "lib",
    "proto",
    "types",
    "README.md",
    "index.ts",
    "tsconfig.json"
  ]
}
 
